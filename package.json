{
  "name": "downdoc",
  "version": "3.0.3",
  "description": "Simple JavaScript documentation generator.",
  "main": "index.js",
  "bin": {
    "downdoc": "bin/downdoc.js"
  },
  "scripts": {
    "prepublish": "npm run docs && npm run example-docs",

    "docs": "./bin/downdoc.js 'index.js' docs",
    "example-docs": "./bin/downdoc.js 'examples/src/**/*.js' examples/docs"
  },
  "keywords": [
    "markdown",
    "documentation",
    "docs"
  ],
  "author": "Talon",
  "license": "MIT",
  "dependencies": {
    "acorn-extract-comments": "^0.2.0",
    "map-stream": "0.0.6",
    "minimist": "^1.1.3",
    "rimraf": "^2.4.2",
    "vinyl-fs": "^1.0.0",
    "xtend": "^4.0.0"
  }
}
